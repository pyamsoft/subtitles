{
  "name": "subsnotass",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:frontend": "vite --config frontend/vite.config.ts frontend",
    "dev:backend": "vite --config backend/vite.config.ts backend",
    "build:frontend": "tsc -b && vite --config frontend/vite.config.ts build frontend",
    "build:backend": "tsc -b && vite --config backend/vite.config.ts build backend",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0 .",
    "format": "prettier --write .",
    "precommit": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@mui/icons-material": "6.1.7",
    "@mui/material": "6.1.7",
    "fastify": "5.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.28.0",
    "uuid": "11.0.3"
  },
  "devDependencies": {
    "@eslint/js": "9.15.0",
    "@types/node": "22.9.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/uuid": "10.0.0",
    "@vitejs/plugin-react-swc": "3.7.1",
    "eslint": "9.15.0",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0-rc.0",
    "eslint-plugin-react-refresh": "0.4.14",
    "globals": "15.12.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "npm-check-updates": "17.1.11",
    "prettier": "3.3.3",
    "typescript": "5.6.3",
    "typescript-eslint": "8.14.0",
    "vite": "5.4.11"
  },
  "packageManager": "pnpm@9.13.2",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,html}": [
      "eslint --report-unused-disable-directives --max-warnings 0",
      "prettier --write"
    ]
  }
}
